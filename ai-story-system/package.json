{
  "name": "ai-story-system",
  "version": "1.0.0",
  "description": "AI-Powered Interactive Story Generation System using TypeScript and Express",
  "private": true,
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc",
    "test": "jest --testPathIgnorePatterns=e2e",
    "test:unit": "jest --testPathIgnorePatterns=e2e",
    "test:e2e": "cross-env NODE_ENV=test jest --config jest-e2e.config.js",
    "test:all": "pnpm test && pnpm test:e2e",
    "test:watch": "jest --watch --testPathIgnorePatterns=e2e",
    "test:e2e:watch": "cross-env NODE_ENV=test jest --config jest-e2e.config.js --watch",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "db:migrate": "pnpm exec typeorm-ts-node-commonjs migration:run -d src/database/data-source.ts",
    "db:migrate:dev": "pnpm typeorm:dev migration:run -d src/database/data-source.ts",
    "db:migrate:revert": "pnpm exec typeorm-ts-node-commonjs migration:revert -d src/database/data-source.ts",
    "db:migrate:revert:dev": "pnpm typeorm:dev migration:revert -d src/database/data-source.ts",
    "db:migrate:generate": "pnpm exec typeorm-ts-node-commonjs migration:generate -d src/database/data-source.ts",
    "db:migrate:show": "pnpm exec typeorm-ts-node-commonjs migration:show -d src/database/data-source.ts",
    "typeorm": "pnpm exec typeorm-ts-node-commonjs",
    "typeorm:dev": "pnpm exec ts-node ./node_modules/typeorm/cli.js"
  },
  "keywords": [],
  "author": "Thiago Trzcinski",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.24.3",
    "@google/generative-ai": "^0.15.0",
    "axios": "^1.6.0",
    "bcrypt": "^6.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "elevenlabs": "^0.8.1",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "openai": "^4.52.7",
    "pg": "^8.11.0",
    "reflect-metadata": "^0.1.13",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "tsyringe": "^4.8.0",
    "typeorm": "^0.3.25",
    "uuid": "^9.0.1",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/cors": "^2.8.17",
    "@types/express": "4.17.21",
    "@types/jest": "^29.5.11",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/lodash": "^4.17.20",
    "@types/node": "^20.10.0",
    "@types/pg": "^8.15.4",
    "@types/supertest": "^6.0.3",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.6",
    "@types/uuid": "^9.0.7",
    "cross-env": "^10.1.0",
    "http-status": "^2.1.0",
    "jest": "^29.7.0",
    "prettier": "^3.5.3",
    "supertest": "^7.1.4",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.2"
  },
  "packageManager": "pnpm@9.0.0+sha512.b4106707c7225b1748b61595953ccbebff97b54ad05d002aa3635f633b9c53cd666f7ce9b8bc44704f1fa048b9a49b55371ab2d9e9d667d1efe2ef1514bcd513"
}
